<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="account_wht_wizard_form" model="ir.ui.view">
			<field name="name">account.wht.wizard.form</field>
			<field name="model">account.wht.wizard</field>
			<field name="arch" type="xml">
				<form string="Generate Withholding Moves" version="7.0">
					<separator string="Generate Withholding Moves" name="product_separator" />
					<group>
						<field name="number" />
					</group>
					<footer>
						<button name="button_confirm" string="Confirm" type="object"
							class="oe_highlight" />
						or
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</openerp>