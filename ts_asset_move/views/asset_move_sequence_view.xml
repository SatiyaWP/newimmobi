<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">
        <!-- Sequences for asset move --> 
        <record id="seq_type_asset_move" model="ir.sequence.type">
            <field name="name">asset_move_sequence_type</field>
            <field name="code">asset.move.seq</field>
        </record>
        <record id="seq_asset_move" model="ir.sequence">
            <field name="name">Asset Move Sequence</field>
            <field name="code">asset.move.seq</field>
            <field name="suffix">.1.AM.%(month)s.%(year)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
