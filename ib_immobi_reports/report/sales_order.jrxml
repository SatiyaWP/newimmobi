					<rightPen lineWidth="0.5"/>
				</box>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[($P{persen_total}.isEmpty() ?
    "0 %" : $P{persen_total} +"%")]]></textFieldExpression>
			</textField>
		</band>
	</lastPageFooter>
</jasperReport>
